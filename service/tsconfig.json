{
    "compilerOptions": {
        "types": [
            "node"
        ],
        "module": "CommonJS",
        "allowJs": false,
        "target": "esnext",
        "useDefineForClassFields": true,
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": false,
        "resolveJsonModule": false,
        "isolatedModules": true,
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "DOM.Iterable",
            "dom"
        ],
        "baseUrl": "/",
    },
    "exclude": [],
    "include": [
        "."
    ]
}